{
  "name": "Instagram : Autolike !",
  "background": {
        "scripts": [
            "bg/background.js"
        ]
    },
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "very simple Macro Autolike for Instagram",
  "homepage_url": "https://www.youtube.com/c/freeangelz?sub_confirmation=1",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Tiktok unfollower",
    "default_popup": "popup/browser_action.html"
  },
  "permissions": [
    "background",
    "http://*.instagram.com/*",
    "https://instagram.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
	"js/jquery.min.js",
        "inject.js",
		"accounts.js",
		"comments.js"
      ]
    }
  ]
}